{
  "name": "redash-client",
  "description": "Redash API Client for JavaScript",
  "version": "2.0.0",
  "author": "Teppei Sato <teppeis@gmail.com>",
  "engines": {
    "node": ">=8"
  },
  "main": "lib/RedashClient.js",
  "types": "lib/RedashClient.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "docs": "run-s docs:clean docs:build",
    "docs:build": "typedoc --out docs --exclude '**/src/test/**' src",
    "docs:clean": "rimraf docs",
    "lint": "tslint -c tslint.js src/**/*.ts",
    "prepublishOnly": "run-p test docs",
    "test": "NOCK_BACK_MODE=lockdown run-s lint tsc test:unit",
    "test:unit": "mocha --require espower-typescript/guess src/test/*.ts",
    "tsc": "run-s tsc:clean tsc:build",
    "tsc:build": "tsc",
    "tsc:clean": "rimraf lib"
  },
  "dependencies": {
    "@types/node": "^8.10.37",
    "cross-fetch": "^2.2.3",
    "delay": "^4.1.0"
  },
  "devDependencies": {
    "@teppeis/nock-back-mocha": "^3.0.0",
    "@types/mocha": "^5.2.5",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-node": "^1.0.3",
    "esdoc-standard-plugin": "^1.0.0",
    "esdoc-typescript-plugin": "^1.0.1",
    "eslint-config-teppeis": "^8.3.1",
    "espower-typescript": "^9.0.1",
    "intelli-espower-loader": "^1.0.1",
    "mocha": "^5.2.0",
    "nock": "^10.0.2",
    "npm-run-all": "^4.1.3",
    "power-assert": "^1.6.1",
    "prettier": "~1.15.1",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-import-eslint-config": "^1.0.2",
    "tslint-plugin-prettier": "^2.0.1",
    "typedoc": "^0.13.0",
    "typescript": "^3.1.6"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:teppeis/redash-client.git"
  },
  "keywords": [
    "redash"
  ],
  "license": "MIT"
}
